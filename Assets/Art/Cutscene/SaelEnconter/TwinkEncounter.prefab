    mirror: 1
    backGround: {fileID: 0}
    eventSpawn: {fileID: 0}
    music: {fileID: 0}
    sfx: {fileID: 0}
    effect: 
  - name: Siea
    lines: I took a step back apon hearing the voice of a human.
    mirror: 0
    backGround: {fileID: 0}
    eventSpawn: {fileID: 0}
    music: {fileID: 0}
    sfx: {fileID: 0}
    effect: 
